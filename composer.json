{
    "name": "compucie/congressus",
    "description": "Library for the Congressus API.",
    "version": "2.5.0",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nathan Djojomoenawie",
            "email": "nathan21100@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Compucie\\Congressus\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "guzzlehttp/guzzle": "^7.8",
        "league/oauth2-client": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "11.5.46",
        "phpstan/phpstan": "2.1.33"
    },
    "scripts": {
        "phpstan-base" : "vendor/bin/phpstan analyse src tests -b -l 8 --allow-empty-baseline",
        "phpstan-check" : "vendor/bin/phpstan analyse -c phpstan.neon",
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/phpunit --teamcity"
        ]
    }
}
